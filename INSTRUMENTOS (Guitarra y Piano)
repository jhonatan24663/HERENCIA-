class Instrumento:
    def __init__(self, nombre, material):
        self.nombre = nombre
        self.material = material
    
    def sonido(self):
        print("Este instrumento produce un sonido musical.")

class Guitarra(Instrumento):
    def tocar(self):
        print("Sonido de guitarra: strum strum")

class Piano(Instrumento):
    def tocar(self):
        print("Sonido de piano: plim plim")
